#include <stdio.h>

int main()
{
    int k,l,m;
    printf("How many chimera ants?");
    scanf("%d %d",&k,&l);
    
    m=k+l;
    printf("The number of chimera ants is %d",m);
    
    if (m>2)
    {
        printf("You will surely die.");
    }
    
    else if (m=1)
    {
        printf("Run away quickly.");
    }
    
    else
    {
        printf("You are rucky.");
    }
    
    return 0;
    
}
